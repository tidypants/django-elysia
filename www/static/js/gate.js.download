ROITracker.getQueryParameterCustom = function(name) {
    var value = ROITracker.getQueryParameter(name);
    if (value === "true") {
        return true;
    } else if (value === "false") {
        return false;
    } else if (value === "NULL") {
        return null;
    }
    return value;
};

ROITracker.getDataLayerVariable = function(name) {
    var gtm;

    if (google_tag_manager) {
        gtm = google_tag_manager['GTM-5KHVDH'];
        return gtm.dataLayer.get(name);
    }

    return null;
};

ROITracker.getRandomId = function () {
    return (new Date()).getTime() + (Math.floor(Math.random() * 900) + 100).toString();
};

adBlockEnabled = function() {
	var ad = document.createElement('ins');
  	ad.className = 'AdSense';
  	ad.style.display = 'block';
  	ad.style.position = 'absolute';
  	ad.style.top = '-1px';
  	ad.style.height = '1px';
  	document.body.appendChild(ad);
  	var isAdBlockEnabled = !ad.clientHeight;
  	document.body.removeChild(ad);
  	return isAdBlockEnabled;
}


 /***
 * End of Auxiliary Library
 *
 * Begin Client Script
 ***/

pageTracker = new ROITracker("UA-116144929-1", "");
pageTracker.createTracker("UA-116144929-2", "t3").setAttributionModel(true, true, "first_cpc");

var adBlockStatus = adBlockEnabled();
pageTracker.setCustomData('dimension3', adBlockStatus);
pageTracker.setCustomData('dimension4', ROITracker.getDataLayerVariable('pageType'));


pageTracker._setDomainName("astrologyanswers.com");

if (ROITracker.getQueryParameter('__roiNonDirect')) {
    ROITracker.setCookieAutoDomain('__roiNonDirect', 'true');
}

if (ROITracker.getQueryParameter('__roiCpc')) {
    ROITracker.setCookieAutoDomain('__roiCpc', 'true');
}

if (ROITracker.getQueryParameter('cid')) {
    pageTracker.setClientId(ROITracker.getQueryParameter('cid'));
}

if (ROITracker.getQueryParameter('from')) {
    pageTracker.setCustomData('dimension1', ROITracker.getQueryParameter('from'));
}

pageTracker._trackPageview(false);

ROITracker.trackAdWordsRemarketing("994832602", "3FDICL6o1gYQ2uGv2gM");
ROITracker.processQueuedCommands();

// Remarketing cookie
(function() {
    function getValue(param1, param2) {
        return ROITracker.getQueryParameterCustom(param1) || ROITracker.getQueryParameterCustom(param2) || '';
    }

    var value = 'campaign=' + getValue('utm_campaign', 'ga_campaign');
    value += '|adgroup=' + getValue('utm_content', 'ga_adgroup');
    value += '|keyword=' + getValue('utm_term', 'ga_keyword');
    value += '|creative=' + getValue('utm_creative', 'ga_creative');

    if (ROITracker.getQueryParameterCustom('roiremarketing')) {
        ROITracker.setCookie('roiRemarketing', value, 30, 'astrologyanswers.com');
    }
})();

// Ecommerce tracking
(function() {
    var $_GET = ROITracker.getQueryParameterCustom;
    var order = {};

    order.id = $_GET('orderId') || ROITracker.getRandomId();
    order.amount = $_GET('orderAmount') || 0;
    order.affiliate = "";
    order.tax = 0;
    order.shipping = 0;
    order.city = $_GET('city') || "";
    order.state = $_GET('state') || "";
    order.country = $_GET('country') || "";
    order.itemid = $_GET('productId') || 1000;
    order.itemname = ($_GET('productName') || 'UNKNOWN').replace(/\++/g, ' ');
    order.category = "";
    order.quantity = 1;

    if(order.amount !== 0) {
        pageTracker._addTrans(order.id,order.affiliate,order.amount,order.tax,order.shipping,order.city,order.state,order.country);
        pageTracker._addItem(order.id,order.itemid,order.itemname,order.category,order.amount,order.quantity);
        pageTracker._trackTrans();
    }
})();
